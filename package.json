{
  "name": "knotta-lsp-vscode-ext",
  "version": "1.0.2",
  "description": "VSCode extension for knotta lsp client",
  "main": "dist/index.js",
  "engines": {
    "vscode": "^1.94.0"
  },
  "files": [
    "dist",
    "LICENSE"
  ],
  "activationEvents": [
    "onLanguage:knotta"
  ],
  "contributes": {
    "languages": [
      {
        "id": "knotta",
        "extensions": [
          ".knotta"
        ]
      }
    ]
  },
  "scripts": {
    "build": "tsx build-scripts/build.ts",
    "watch": "tsx build-scripts/watch.ts",
    "start": "node dist",
    "package": "vsce package --no-dependencies",
    "publish": "vsce publish --no-dependencies"
  },
  "publisher": "coder-ka",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/coder-ka/knotta-lsp-vscode-ext.git"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/coder-ka/knotta-lsp-vscode-ext/issues"
  },
  "homepage": "https://github.com/coder-ka/knotta-lsp-vscode-ext#readme",
  "devDependencies": {
    "@types/node": "^18.11.9",
    "@types/vscode": "^1.94.0",
    "@vscode/vsce": "^3.1.1",
    "chokidar": "^4.0.1",
    "esbuild": "^0.15.15",
    "knotta-lsp-server": "^1.0.0",
    "tsx": "^4.19.1"
  },
  "dependencies": {
    "vscode-languageclient": "^9.0.1"
  }
}
